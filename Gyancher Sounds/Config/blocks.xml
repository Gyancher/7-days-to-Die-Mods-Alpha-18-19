<configs>
	<append xpath="/blocks">



<block name="+music underground">
			<property name="CustomIcon" value="sound" />
			<property name="Material" value="Mmushrooms" />
			<property name="Shape" value="ModelEntity" />
			<property name="Mesh" value="models" />
			<property name="Model" value="#@modfolder:Resources/underground.unity3d?underground.prefab" />
			<property name="Collide" value="movement,melee,bullet,arrow,rocket" />
			<property name="CanPickup" value="true" param1="+music underground" />
			<property name="IsTerrainDecoration" value="true" />
			<property name="HarvestOverdamage" value="false" />
			<drop event="Destroy" name="+music underground" count="1" />
		</block>
		
		
