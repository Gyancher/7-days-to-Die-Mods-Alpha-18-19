<block name="+Angel Statue">
	<property name="Material" value="Mconcrete" />
	<property name="CustomIcon" value="GargoylStatue" />
	<property name="Shape" value="ModelEntity"/>
	<property name="Model" value="#@modfolder:Resources/angel.unity3d?Angel.prefab"/>
	<property name="Collide" value="movement,melee,bullet,arrow,rocket"/>
